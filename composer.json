{
    "name": "vanthao03596/laravel-hanhchinhvn",
    "description": "hành chính việt nam laravel",
    "keywords": [
        "vanthao03596",
        "hanhchinhvn",
        "laravel"
    ],
    "homepage": "https://github.com/vanthao03596/laravel-hanhchinhvn",
    "license": "MIT",
    "authors": [
        {
            "name": "PhamThao",
            "email": "phamthao03596@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Vanthao03596\\HCVN\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanthao03596\\HCVN\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vanthao03596\\HCVN\\HCVNServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
